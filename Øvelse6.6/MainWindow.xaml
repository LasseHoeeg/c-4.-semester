<Window x:Class="Øvelse6._6.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Øvelse6._6"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <Button DockPanel.Dock="Left" Width="74">
            <Button Name="West" Content="West" Height="50" Width="50"/>
        </Button>
        <Button DockPanel.Dock="Top" Height="88">
            <Button Name="North" Content="North" Height="50" Width="50"/>
        </Button>
        <Button DockPanel.Dock="Right" Width="95">
            <Button Name="East" Content="East" Height="50" Width="50"/>
        </Button>
        <Button DockPanel.Dock="Bottom" Height="76">
            <Button Name="South" Content="South" Height="50" Width="50"/>
        </Button>
        <Button>
            <TextBox Name="TextBox" Height="140" TextAlignment="Center" TextWrapping="Wrap"  Width="298"/>
        </Button>
    </DockPanel>
</Window>
